<?xml version="1.0" encoding="UTF-8"?>
<mc:ModelConfig  
  xmlns:mc="http://www.modeldriven.org/fuml/meta/config">
  
    <artifact name="Infrastructure.uml" factoryClassName="org.modeldriven.fuml.meta.InfrastructureAssembler"/>   
    <artifact name="Superstructure.uml" factoryClassName="org.modeldriven.fuml.meta.SuperstructureAssembler"/>
    <artifact name="fUML.uml" factoryClassName="org.modeldriven.fuml.meta.fUMLAssembler">
        <registeredPackage name="fUML.Syntax"/>
        <registeredPackage name="fUML.Semantics"/>
        <registeredPackage name="fUML.Library"/>
        <registeredPackage name="fUML.Test"/>
    </artifact>    
    <artifact name="fUML-LibraryImplementation.uml" factoryClassName="org.modeldriven.fuml.meta.fUML_LibraryImplementationAssembler">
        <registeredPackage name="org.modeldriven.fuml.library"/>
    </artifact>    

    <ignoredPackage name="UML.AuxiliaryConstructs.Models"/>
    <ignoredPackage name="InfrastructureLibrary.Profiles"/>
    <ignoredPackage name="InfrastructureLibrary.Core.Constructs"/>
    <ignoredPackage name="fUML.Ignored"/>

    <ignoredClass name="PackageImport"/>
  
</mc:ModelConfig>   
