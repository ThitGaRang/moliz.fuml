<?xml version="1.0" encoding="UTF-8"?>
<cfg:Configuration  
  xmlns:cfg="http://www.modeldriven.org/fuml/config">
  
  <ImportConfiguration>
  
    <!-- supported namespaces -->
    <SupportedNamespace domain="xmi" uri="http://schema.omg.org/spec/XMI/2.1" />
    <SupportedNamespace domain="uml" uri="http://www.eclipse.org/uml2/2.0.0/UML" />
    <SupportedNamespace domain="uml" uri="http://schema.omg.org/spec/UML/2.1.1" />
    <SupportedNamespace domain="uml" uri="http://www.eclipse.org/uml2/2.1.0/UML" />
    <SupportedNamespace domain="uml" uri="http://schema.omg.org/spec/UML/2.1.2" />    
    
    <SupportedNamespace domain="ecore" uri="http://www.eclipse.org/emf/2002/Ecore" />
    <SupportedNamespace domain="magicdraw" uri="http:///schemas/MagicDrawProfile/_eFzsIKxbEd2Kp9hnZinLyQ/0" />
  
    <!-- ignored elements -->
    <Element localName="Extension" domain="xmi" ignored="true" />    
    <Element localName="packageImport" domain="uml" ignored="true" />
    <Element localName="profileApplication" domain="uml" ignored="true" />
    <Element localName="eAnnotations" domain="uml" ignored="true" />    

    <!-- adapters -->
    <Adapter type="assembly" className="OpaqueExpression" adapterClassName="org.modeldriven.fuml.assembly.OpaqueExpressionAdapter"/>    
    
  </ImportConfiguration>
  
  <MappingConfiguration>
    <ReferenceMapping type="parent" className="Generalization" propertyName="specific"/>   
    
    <BehaviorExecutionMapping className="fUML_Library.BasicInputOutput.StandardOutputChannel" executionClassName="StandardOutputChannelObject"/>

    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.Div" executionClassName="IntegerDivideFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.*" executionClassName="IntegerTimesFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.-" executionClassName="IntegerMinusFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.+" executionClassName="IntegerPlusFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.Neg" executionClassName="IntegerNegateFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.Max" executionClassName="IntegerMaxFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.Min" executionClassName="IntegerMinFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.Mod" executionClassName="IntegerModFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.Abs" executionClassName="IntegerAbsFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.ToInteger" executionClassName="IntegerToIntegerFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.ToString" executionClassName="IntegerToStringFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.ToUnlimitedNatural" executionClassName="IntegerToUnlimitedNaturalFunctionBehaviorExecution"/>

    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.&lt;" executionClassName="IntegerLessThanFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.&lt;=" executionClassName="IntegerLessThanEqualFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.&gt;" executionClassName="IntegerGreaterThanFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.IntegerFunctions.&gt;=" executionClassName="IntegerGreaterThanEqualFunctionBehaviorExecution"/>

    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.BooleanFunctions.ToBoolean" executionClassName="BooleanToBooleanFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.BooleanFunctions.ToString" executionClassName="BooleanToStringFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.BooleanFunctions.Or" executionClassName="BooleanOrFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.BooleanFunctions.Xor" executionClassName="BooleanXorFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.BooleanFunctions.And" executionClassName="BooleanAndFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.BooleanFunctions.Not" executionClassName="BooleanNotFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.BooleanFunctions.Implies" executionClassName="BooleanImpliesFunctionBehaviorExecution"/>

    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.StringFunctions.Size" executionClassName="StringSizeFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.StringFunctions.Concat" executionClassName="StringConcatFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.StringFunctions.Substring" executionClassName="StringSubstringFunctionBehaviorExecution"/>

    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.&lt;" executionClassName="UnlimitedNaturalLessThanFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.&lt;=" executionClassName="UnlimitedNaturalLessThanEqualFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.Max" executionClassName="UnlimitedNaturalMaxFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.Min" executionClassName="UnlimitedNaturalMinFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.&gt;" executionClassName="UnlimitedNaturalGreaterThanFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.&gt;=" executionClassName="UnlimitedNaturalGreaterThanEqualFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.ToUnlimitedNatural" executionClassName="UnlimitedNaturalToUnlimitedNaturalFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.ToString" executionClassName="UnlimitedNaturalToStringFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.UnlimitedNaturalFunctions.ToInteger" executionClassName="UnlimitedNaturalToIntegerFunctionBehaviorExecution"/>

    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.ListFunctions.ListGet" executionClassName="ListGetFunctionBehaviorExecution"/>
    <BehaviorExecutionMapping className="fUML_Library.PrimitiveBehaviors.ListFunctions.ListSize" executionClassName="ListSizeFunctionBehaviorExecution"/>

  </MappingConfiguration>

  <LibraryConfiguration>
    <LibraryImport name="fUML_Library.uml" baseURI="fUML-Library.mdzip" delimiter="#"/>
  </LibraryConfiguration>
  
</cfg:Configuration>   
